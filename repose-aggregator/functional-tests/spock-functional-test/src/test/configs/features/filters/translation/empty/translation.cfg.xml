<?xml version="1.0" encoding="UTF-8"?>

<translation xmlns="http://docs.openrepose.org/repose/translation/v1.0" allow-doctype-decl="true">
    <request-translations>
        <request-translation content-type="application/xml" translated-content-type="application/xml">
            <style-sheets>
                <style id="copy">
                    <xsl>
                        <xsl:stylesheet
                                xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                                xmlns:httpx="http://docs.openrepose.org/repose/httpx/v1.0"
                                version="2.0">

                            <xsl:output method="xml" encoding="UTF-8"/>

                            <xsl:template match="node() | @*">
                                <xsl:copy>
                                    <xsl:apply-templates select="@* | node()"/>
                                </xsl:copy>
                            </xsl:template>
                        </xsl:stylesheet>
                    </xsl>
                </style>
            </style-sheets>
        </request-translation>
    </request-translations>
</translation>
